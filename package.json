{
  "name": "@ianhatton/vanilla-carousel",
  "version": "1.0.2",
  "author": "Ian Hatton",
  "description": "Carousel module written in vanilla JavaScript",
  "keywords": [
    "vanilla carousel"
  ],
  "bugs": {
    "url": "https://github.com/ianhatton/vanilla-carousel/issues"
  },
  "homepage": "https://github.com/ianhatton/vanilla-carousel#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhatton/vanilla-carousel.git"
  },
  "license": "MIT",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital"
  },
  "scripts": {
    "build": "babel src -x '.js' --source-maps  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari",
    "test:watch": "karma start --browsers PhantomJS"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "2.13.0",
    "eslint-config-google": "0.6.0",
    "eslint-config-morsedigital": "1.0.4",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^5.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-story-reporter": "^0.3.1",
    "lodash": "^4.6.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rewireify": "^0.2.5",
    "viewport-detection-es6": "~2.1.*",
    "watchify": "^3.7.0"
  },
  "peerDependencies": {
    "lodash": "^4.6.0",
    "viewport-detection-es6": "~2.1.*"
  }
}
